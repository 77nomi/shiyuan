.whole{
  width: 650rpx;
  height: 200rpx;
  background-color: rgb(255, 255, 255);
  margin:10rpx auto 20rpx;
  border-radius: 30rpx;
}
.simples{
  display: flex;
  align-items: center;
  height: 115rpx;
}
.biaoqian{
  margin:0 15rpx;
  font-size: 40rpx;
}
.title{
  font-size: 40rpx;
}
.details{
  display: flex;
  justify-content: space-around;
  align-items: center;
  height: 65rpx;
}
.name{
  margin-left: 10rpx;
  text-align: center;
}
.states{
  height: 50rpx;
  width: 100rpx;
  text-align: center;
  font-size: 35rpx;
  background-color: #359BB0;
  border-radius: 10rpx;
}
.time{
  font-size: 25rpx;
  border-radius: 30rpx;
  background-color: #fff;
}

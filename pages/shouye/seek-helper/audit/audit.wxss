.rect1 {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  background-color: #C5DFE8;
}
.rect2 {
  position: fixed;
  top: 20px;
  left: 20px;
  width: calc(100% - 40px);
  height: calc(100vh - 40px);
  border-radius: 20px;
  background-color: #E5F1F5;
}
.rect3 {
  position: fixed;
  top: 40px;
  left: 40px;
  width: calc(100% - 80px);
  height: calc(100vh - 80px);
  background-color: white;
}

 
.row1{
  width: 500rpx;
  height: 120rpx;
  display: flex;
  border-radius: 20px;
  margin: 0 auto;
  margin-top: 50rpx;
  display: flex;
  align-items: center;
}
.left{
  width: 140rpx;
  display: flex;
  align-items: center;
}
image{
  width: 120rpx;
}
.name{
  width: 200rpx;
  height: 120rpx;
  line-height: 120rpx;
  margin-left: 20rpx;
  font-size: 40rpx;
}
.finished{
  width: 130rpx;
  height: 50rpx;
  background-color:yellow;
  line-height: 50rpx;
  text-align: center;
  border-radius:8px;
}
.content{
  padding: 20rpx;
  height: 500rpx;
  width: 500rpx;
  margin: 10px auto 0;
  border:2rpx solid black;
}
.row2{
  margin: 10px auto;
  border:5rpx solid black;
}  
.row3{
  display: flex;
  justify-content: space-between;
  align-items:center;
  width: 500rpx;
  margin: 10px auto;
}
.button{
  width: 180rpx;
  height: 80rpx;
  margin-top: 50rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 40rpx;
  border:5rpx solid #FF8D1A;
}
.picsbox{
  border:2rpx solid black;
  border-top: none;
  margin: 0 auto;
  width: 540rpx;
  height: 200rpx;
  white-space: nowrap;
}
.pics{
  width: 190rpx;
  height: 190rpx;
  margin: 5rpx;
  display: inline-block;
}
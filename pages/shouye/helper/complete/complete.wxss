
.rect1 {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  background-color: #C5DFE8;
}
.rect2 {
  position: fixed;
  top: 40rpx;  
  left: 40rpx;  
  width: calc(100% - 80rpx);  
  height: calc(100vh - 80rpx);  
  border-radius: 30rpx;  
  background-color: #E5F1F5;
}
.container1{
  width: 550rpx;
  height: 150rpx;
  background-color:white;
  border-radius: 30rpx;
  padding-left: 20rpx;
  margin: 0 auto;
  margin-top: 60rpx;
  display: flex;
  flex-direction: column;
}
.title,
.faburen,
.id{
  height: 50rpx;
  line-height: 50rpx;

}
.row1{
  width: 530rpx;
  height: 120rpx;
  display: flex;
  border-radius: 30rpx;
  background-color: rgb(255, 255, 255);
margin: 0 auto;
margin-top: 120rpx;
}

.container2 {
  width:  550rpx;
  background-color: white;
  border-radius: 30rpx;
  margin: 50rpx auto;
}
.input {
  /* flex: 1; */

.rect1 {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  background-color: #C5DFE8;
}
.rect2 {
  position: fixed;
  top: 30rpx;  
  left: 30rpx;  
  width: calc(100% - 60rpx);  
  height: calc(100vh - 60rpx);  
  border-radius: 30rpx;  
  background-color: #E5F1F5;
}
.information
{ height: 100rpx;
  background-color: #fff;
  border-radius: 30rpx;  
  line-height: 100rpx;
  text-align: center;
  font-size: 40rpx;
}


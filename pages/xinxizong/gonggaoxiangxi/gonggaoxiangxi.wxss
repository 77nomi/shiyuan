.rect1 {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  background-color: #C5DFE8;
}
.rect2 {
  position: fixed;
  top: 40rpx;  
  left: 40rpx;  
  width: calc(100% - 80rpx);  
  height: calc(100vh - 80rpx);  
  border-radius: 30rpx;  
  background-color: #E5F1F5;
}


.all {
  width: 100%;
  height: 100vh;
  background-color: #C5DFE8;
  display: flex;
  justify-content: center;
  align-items: center;
}
.container {
  position: relative;
  /* padding: 0; */
  width: calc(100% - 30px);
  height: calc(100% - 30px);
  border-radius: 20px;
  background-color: #E5F1F5;
  overflow: auto;
}
